{
  "name": "game-review-api",
  "version": "1.0.0",
  "description": "API REST de avaliação de jogos com Node.js, TypeScript, Prisma, JWT e Swagger.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --transpile-only src/server.ts",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:generate:prod": "prisma generate --schema prisma/postgres/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:migrate:prod": "prisma migrate deploy --schema prisma/postgres/schema.prisma",
    "prisma:studio": "prisma studio",
    "test": "jest"
  },
  "author": "Marcus Vinícius",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.19.3",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "prisma": "^6.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
